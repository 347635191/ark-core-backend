<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.best.statistics.domain.stat.team.mapper.TeamBattleRecordMapper">
    <sql id="base_column">
        id, enemy_team_name, battle_date, state, team_rank
    </sql>

    <select id="queryAll" resultType="org.best.statistics.domain.stat.team.entity.TeamBattleRecord">
        SELECT
        <include refid="base_column"/>
        FROM team_battle_record_t
    </select>
</mapper>