<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.best.statistics.domain.stat.score.mapper.UserBattleScoreMapper">
    <sql id="base_column">
        id, user_id, name, battle_date, score, complete_attack
    </sql>

    <select id="queryAll" resultType="org.best.statistics.domain.stat.score.entity.UserBattleScore">
        SELECT
        <include refid="base_column"/>
        FROM user_battle_score_t
    </select>
</mapper>