<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.best.statistics.domain.user.mapper.UserMapper">
    <sql id="base_column">
        id, name, state
    </sql>

    <select id="queryAll" resultType="org.best.statistics.domain.user.entity.User">
        SELECT
        <include refid="base_column"/>
        FROM user_info_t
    </select>
</mapper>